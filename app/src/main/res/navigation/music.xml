<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/music_navigation"
    app:startDestination="@id/favoriteListFragment">

    <fragment
        tools:layout="@layout/fragment_favorite_list"
        android:id="@+id/favoriteListFragment"
        android:name="com.vguivarc.wakemeup.transport.favoritelist.FavoriteListFragment"
        android:label="@string/musique" >
        <action
            android:id="@+id/action_favoriteListFragment_to_searchVideoFragment"
            app:destination="@id/searchVideoFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchVideoFragment"
        android:name="com.vguivarc.wakemeup.transport.search.SearchSongListFragment"
        android:label="RechercheVideoFragment" />
</navigation>